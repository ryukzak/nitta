{
  "name": "nitta-web-ui",
  "version": "0.1.0",
  "description": "NITTA Web UI",
  "scripts": {
    "build": "npm run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d build",
    "lint": "tslint './src/**/*.ts*' --format stylish --force",
    "start": "npm run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "npm run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/chart.js": "^2.7.48",
    "@types/foundation-sites": "^6.4.3",
    "@types/jest": "^23.3.14",
    "@types/jquery": "^3.3.29",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "node-sass": "^4.12.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "tslint": "^5.12.0",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-middleware": "^3.5.0",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "chart.js": "^2.7.3",
    "foundation-sites": "^6.5.3",
    "graphviz-react": "^1.0.4",
    "jest": "^23.6.0",
    "jquery": "^3.4.1",
    "react": "^16.8.1",
    "react-chartjs-2": "^2.7.4",
    "react-d3-tree": "^1.12.2",
    "react-dom": "^16.8.1",
    "react-easy-chart": "^1.0.0",
    "react-graph-vis": "^1.0.2",
    "react-table": "^6.9.0",
    "typescript": "^3.2.2"
  }
}
